{
    "name": "unspokenn/roles-module",
    "description": "",
    "keywords": [
        "laravel",
        "roles",
        "module"
    ],
    "license": "MIT",
    "type": "laravel-module",
    "support": {
        "issues": "https://github.com/unspokenn/roles-module/issues",
        "source": "https://github.com/unspokenn/roles-module"
    },
    "authors": [
        {
            "name": "Sadık Ergüven",
            "email": "unspokenn@protonmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0",
        "friendsofphp/php-cs-fixer": "^3.6",
        "laravel/framework": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "phpstan/phpstan": "^1.9"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "main-dev"
        },
        "laravel": {
            "providers": [
                "Modules\\Roles\\src\\Providers\\RolesServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Roles\\": "src/",
            "Modules\\Roles\\Database\\Factories\\": "database/factories/",
            "Modules\\Roles\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Roles\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
